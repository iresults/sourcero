{
    "name": "iresults/sourcero",
    "description": "Source code management from the TYPO3 CMS Backend",
    "keywords": ["typo3cms"],
    "repositories": [
        {
            "type": "git",
            "url": "http://git.iresults.me/Iresults.Core.git/"
        },
        {
            "type": "git",
            "url": "http://git.iresults.me/Iresults.FS.git/"
        },
        {
            "type": "package",
            "package": {
                "name": "marijnh/codemirror",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/marijnh/CodeMirror.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "symfony/process": "*",
        "iresults/fs": "*"
    },
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "cod@iresults.li"
        }
    ],
    "minimum-stability": "dev"
}
